}\n\n#NW { top:0;   left:0;}\n#NE { top:0;   left:50%;}\n#SW { top:50%; left:0;}\n#SE { top:50%; left:50%;}   \n\n.nav-button {\n    position: fixed;\n    z-index: 5 !important;\n\n    border-style: solid;\n    border-width: 2px;\n\n    color: white !important;\n    \n    cursor: pointer;\n    display: inline-block;\n    font-family: 'Roboto', sans-serif;\n    font-size: 0.8rem;\n    font-weight: 900;\n    letter-spacing: 0.075em;\n    height: 3rem;\n    line-height: 3rem;\n    padding: 0 2rem;\n    text-align: center;\n    text-decoration: none;\n    text-transform: uppercase;\n    white-space: nowrap;\n    background-color: rgba(255, 255, 255, 0.1);\n}\n.nav-button div:hover{\n    color: black;\n}\n\n.svg-button {\n    z-index: -1;\n\n    height: 100%;\n    position: absolute;\n    top: 0;\n\n    background-color: white;\n}\n\n/* MARGIN */\n#margin {\n    pointer-events: none;\n    position: fixed;\n    z-index: 1;\n\n    border-style: solid;\n    color: white;\n\n    /* border-width: 2px;\n    width: calc(100vw - (10px + 2px)*2);\n    height: calc(100vh - (10px + 2px)*2);\n    margin: 10px; */\n\n    border-width: 10px;\n    width: calc(100vw - 10px*2);\n    height: calc(100vh - 10px*2);\n    margin: 0px;\n}\n\n\n/* SOCIAL ICONS */\n#social {\n    z-index: 2;\n    position: fixed;\n\n    transform: translate(-50%, 0);\n\n    left: 50vw;\n    bottom: 20px;\n}\n#social a {\n    font-size: 30px;\n\n    margin: 20px;\n\n    text-decoration: none;\n    color: white;\n\n    opacity: 0;\n}\n#social a:hover {\n    /* color: #158DA5; */\n    color: black;\n    filter: drop-shadow(0px 3px 3px rgba(255, 255, 255, 0.1));\n}\n\n\n/* TEXT BUTTONS */\n#contactate {\n    color: white;\n    font-family: 'Roboto', sans-serif;\n    font-size: 1rem;\n    font-weight: 900;\n    letter-spacing: 0.075em;\n\n    border-bottom: 2px solid white;\n    padding-bottom: 3px;\n\n    z-index: 10;\n    display: block;\n    position: absolute;\n    left: calc(50vw - 7.5ch);\n    top: 20px;\n    text-align: center;\n\n    cursor: pointer;\n\n    opacity: 0;\n}\n\n.black-highlight {\n    background-image: linear-gradient(black, black);\n    background-repeat: no-repeat;\n    background-position: right bottom;\n    background-size: 0% 1rem;\n}"},"lineCount":190}},"error":null,"hash":"640aa3b4e73e02f3aab87c69d09ce3d6","cacheData":{"env":{}}}